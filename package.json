{
  "name": "frame-audit-ai",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/frontend",
    "packages/backend"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=packages/frontend & npm run dev --workspace=packages/backend",
    "build": "cd packages/frontend && npm run build",
    "build:all": "npm run build --workspaces",
    "test": "npm run test --workspaces",
    "test:properties": "npm run test:properties --workspaces"
  },
  "devDependencies": {
    "@vercel/node": "^3.0.0",
    "typescript": "^5.3.3"
  }
}
